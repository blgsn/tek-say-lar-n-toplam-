import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
	
		int k;
		int total=0;
		Scanner input=new Scanner(System.in);
		do {
		System.out.print("SayÄ± giriniz:");
		k=input.nextInt();
		if(k%2==1) {
			total+=k;}}
		while(k>0); 
			System.out.println("toplam:" + total);
		}
	}
